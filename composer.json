{
  "name": "iutrace/laravel-multi-databases",
  "description": "Multiple databases connections commands for Laravel",
  "keywords": [
    "laravel",
    "multiple",
    "databases",
    "connections",
    "command"
  ],
  "homepage": "https://github.com/iutrace/laravel-multi-databases",
  "license": "MIT",
  "authors": [
    {
      "name": "Facundo Lezcano",
      "email": "flezcano@iupik.com",
      "homepage": "https://iutrace.com"
    }
  ],
  "require": {
    "php" : "^7.4|^8.0",
    "illuminate/support": "^7.0|^8.0",
    "illuminate/database": "^7.0|^8.0"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "orchestra/testbench": "^6.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Iutrace\\MultiDatabases\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iutrace\\MultiDatabases\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Iutrace\\MultiDatabases\\MultiDatabaseServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
